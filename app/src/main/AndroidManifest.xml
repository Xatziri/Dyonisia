<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/dyonisia_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dyonisia_logo"
        android:supportsRtl="true"
        android:theme="@style/LoginPage"
        tools:targetApi="31">
        <activity
            android:name=".AccomplishmentTracker"
            android:exported="false"
            android:label="@string/title_activity_accomplishment_tracker"
            android:theme="@style/LoginPage" />
        <activity
            android:name=".StartQuiz"
            android:exported="false" />
        <activity
            android:name=".Quiz"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/LoginPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>